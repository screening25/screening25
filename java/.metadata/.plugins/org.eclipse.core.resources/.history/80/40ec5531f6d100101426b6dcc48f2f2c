import java.util.Scanner;
public class boj10952 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		

		for (int i = 1 ; i <= 10 ; i++) {
			
			int A = sc.nextInt();
			int B = sc.nextInt();
			
			int ans = A+B;
			
			if (ans > 0) {
				System.out.println(ans);
			} 
		}
	}

}
